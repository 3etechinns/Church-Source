---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Church Source</title>
  <link href="http://churchsourcerocks.com/"/>
  <link type="application/atom+xml" rel="self" href="http://churchsourcerocks.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://churchsourcerocks.com/</id>
  <author>
    <name>Paul Bailey</name>
    <email>paul.m.bailey@gmail.com</email>
  </author>
  {% for post in site.posts|limit:25 %}
  <entry>
    <id>http://churchsourcerocks.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://churchsourcerocks.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Paul Bailey</name>
      <uri>http://churchsourcerocks.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>